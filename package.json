{
  "name": "taskboard",
  "version": "0.1.0",
  "private": true,
  "description": "TaskBoard website",
  "main": "public/index.html",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "start": "npm run serve",
    "dev": "npm run start",
    "build": "npm run clean && cross-env NODE_ENV=production vue-cli-service build",
    "dist": "npm run build",
    "clean": "rimraf dist/",
    "lint": "vue-cli-service lint",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit --no-cache --runInBand",
    "test:watch": "npm run test:unit:watch",
    "test:unit:watch": "vue-cli-service test:unit --watch --no-cache --runInBand",
    "unit:coverage": "vue-cli-service test:unit --coverage --no-cache --runInBand"
  },
  "author": "Richard Shephard",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "axios": "^0.18.0",
    "buefy": "^0.6.5",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.24",
    "axios-mock-adapter": "^1.15.0",
    "cross-env": "^5.1.6",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^3.0.1",
    "vue-template-compiler": "^2.5.17"
  }
}
